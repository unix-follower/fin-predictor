{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "json_formatter": {
      "()": "org_example_fin_predictor.config.json_log_formatter.JsonLogFormatter"
    }
  },
  "handlers": {
    "json_handler": {
      "class": "logging.StreamHandler",
      "stream": "ext://sys.stdout",
      "formatter": "json_formatter"
    }
  },
  "loggers": {
    "root": {
      "level": "DEBUG",
      "handlers": ["json_handler"]
    },
    "gunicorn.error": {
      "level": "DEBUG",
      "handlers": ["json_handler"]
    }
  }
}